<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.navagitionapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.NavagitionApp.NoActionBar">
        <activity android:name=".Activity.CartActivity"></activity>
        <activity android:name=".Activity.ProductDetailsActivity" />
        <activity android:name=".Activity.FormalShirtActivity" />
        <activity android:name=".Activity.Main2Activity" />
        <activity android:name=".Activity.LogInActivity" />
        <activity android:name=".Activity.RegisterActivity" />
        <activity android:name=".Activity.ForgotPasswordActivity" />
        <activity android:name=".Activity.CustomerSupportActivity" />
        <activity android:name=".Activity.GetStartedActivity" />
        <activity android:name=".Activity.AddNewAddressActivity" />
        <activity android:name=".Activity.EditAddressActivity" />
        <activity android:name=".Activity.ChangePasswordActivity" />
        <activity android:name=".Activity.TermsConditionsActivity" />
        <activity android:name=".Activity.FirstActivity" />
        <activity
            android:name=".Activity.MainActivity"
            android:theme="@style/Theme.NavagitionApp.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>